{
    "plugins": [
        "node_modules/better-docs/typescript"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            "src/ISO8583.ts"
        ],
        "exclude": [
            "example/"
        ],
        "includePattern": "\\.(jsx|js|ts|tsx)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./docs/",
        "template": "node_modules/better-docs"
    }
}